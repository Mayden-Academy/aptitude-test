 <table class="table">
     <thead>
         <tr class="d-flex">
             <th scope="col" class="col-1">#</th>
             <th scope="col" class="col-2">Question</th>
             <th scope="col" class="col-2">Answer</th>
             <th scope="col" class="col-2">Correct?</th>
             <th scope="col" class="col-4">Notes</th>
         </tr>
     </thead>
     <tbody>
         {{#each this}}
         <tr
             {{#if correct}}
                 class="rightAnswer"
             {{else}}
                 class="wrongAnswer"
             {{/if}}>
             <td>{{result}}</td>
             <td>{{question}}</td>
             <td>{{userAnswer}}</td>
             <td>
                 {{#if correct}}
                     ✔
                 {{else}}
                     ✕
                 {{/if}}
             </td>
             <td>
             {{#if notesEmpty}}
             <button data-id="{{result}}" class="button-notes"><i class="bi bi-journal-arrow-down"></i></button>
             </td>
         </tr>
         <tr>
             <td id="panel{{result}}" colspan="5" class="panels">
                <p>Notes:<br>{{notes}}</p>
             </td>
         </tr>
         {{/if}}
         {{/each}}
     </tbody>
 </table>
