{{#each data}}
<div class="entry" dataId="{{id}}" dataName="{{name}}" dataEmail="{{email}}" dataTime="{{time}}" dataCanRetake="{{canRetake}}">
    <div dataId="{{id}}" dataName="{{name}}" dataEmail="{{email}}" dataTime="{{time}}" dataTimeAllowed={{timeAllowed}} dataCanRetake="{{canRetake}}"
    {{#if this.topGrade}}
         class="score_item top-grade"
    {{else if this.passingGrade}}
         class="score_item passing-grade"
    {{else if this.fail}}
         class="score_item dubious-grade"
    {{else if this.notTakenYet}}
         class="score_item test-not-taken"
    {{/if}}>
        <button class="btn btn-primary btn_height modalBtn">Edit User</button>
        <button type="button" class="btn btn-danger btn_height">Delete User</button>
        <p class="name">Name: {{this.name}}</p>
        <p class="email">Email: {{this.email}}</p>
        <p class="score">Score: {{this.score}}</p>
        <p class="percentage">Percentage: {{this.percentage}}</p>
        <p class="time">Time: {{this.timeAllowed}}</p>
        <p class="time">Time: {{this.time}}</p>
        <p class="date">Date Test Taken: {{#if this.testNotTaken}}{{this.testNotTaken}}{{else}}{{this.dateCreated}}{{/if}}</p>
    </div>
</div>
{{/each}}