<tr>
    <td>{{id}}</td>
    <td>{{question}}</td>
    {{#if isAnswered}}
    <td class="glyphicon glyphicon-ok"></td>
    {{else}}
    <td class="glyphicon glyphicon-remove"></td>
    {{/if}}
    {{#if isFlagged}}
    <td class="glyphicon glyphicon-flag"></td>
    {{else}}
    <td></td>
    {{/if}}
</tr>