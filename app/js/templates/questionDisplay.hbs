

    <img class="logo" alt="Mayden Logo" src="images/branding-icons/maydenacademy_logotype_05.18_web.svg">
    <div class="row">
        <div class="col-xs-12">
            <div class="container_input display-flex">
                <a href="adminPage.html"><button class="btn primary-button">&#8592; Back</button></a>
                <a href="addQuestion.html"><button class="btn primary-button new-question-button">Add new question</button></a>
            </div>
            <h4 class="search-heading">Current Question Count: <span id="question-count"></span></h4>
            <table class="table question-table">
                <thead>
                    <tr>
                        <th></th>
                        <th>Question ID</th>
                        <th>Question</th>
                        <th>Test ID</th>
                    </tr>
                </thead>
                <tbody class="score_list user-table">
                    {{#each data}}
                        <tr>
                            <td dataId="{{id}}" dataQuestion="{{this.text}}">
                                <button form="submit" class="btn edit-question-button btn_height modalBtn" id="{{this.id}}">Edit Question</button>
                                <button form="submit" class="btn delete-question-button btn_height" id="{{this.id}}">Delete Question</button>
                            </td>
                            <td class="question_id">{{this.id}}</td>
                            <td class="question">{{this.text}}</td>
                            <td class="test_id">{{this.test_id}}</td>
                        </tr>
                    {{/each}}
                </tbody>
            </table>
        </div>
    </div>
