<div class="panel panel-default">
    <div class="panel-heading" role="tab" id="headingOne">
        <h2 class="panel-title">
            <a role="button" data-toggle="collapse" data-parent="#accordion" href="#{{this.testId}}">{{this.testname}}
            </a>
        </h2>
    </div>
    <div id="{{this.testId}}" class="panel-collapse collapse in" role="tabpanel" aria-labelledby="headingOne" data-parent="#accordionExample">
        <div class="panel-body">
            <table class="table">
                <thead>
                <tr>
                    <th class="colBtn">Modify Question</th>
                    <th class="colNum">Number</th>
                    <th>Question</th>
                </tr>

                {{#each data}}
                    <tr>
                        <td data-id="{{id}}" data-question="{{text}}" class="btnContainer">
                            <button class="btn btn_height edit-question">Edit</button>
                        </td>
                        <td class="questionNumber">{{this.id}}</td>
                        <td class="questionText">{{this.text}}</td>
                    </tr>
                {{/each}}
                </thead>
                <tbody class="tableBody tableList"></tbody>
            </table>
        </div>
    </div>
</div>